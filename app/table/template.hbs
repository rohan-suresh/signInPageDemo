<div class="outside">
  <div class="chapters">
    {{#each model.chapters as |tableUnit|}}
      {{render-contents chapter=tableUnit clickedAction=(action 'postData')}}
    {{/each}}
  </div>
    <div class="actions">
        <div> <table>
        <tr>
          <th>Chapter Number</th>
          <th>Section Number</th>
          <th>User ID</th>
        </tr>
          {{#each userActivity as |activity|}}
          <tr>
            <td>{{activity.chapter_number}}</td>
            <td>{{activity.section_number}}</td>
            <td>{{activity.user_id}}</td>
          </tr>
          {{/each}}
        </table> </div>
    </div>
</div>